<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="300.0" maxWidth="400.0" minHeight="300.0" minWidth="400.0" prefHeight="300.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="scp.log.view.LoginController">
    <children>
        <TextField fx:id="Unamefield" layoutX="201.0" layoutY="103.0"/>
        <Label layoutX="69.0" layoutY="107.0" prefHeight="17.0" prefWidth="83.0" text="Login"
               AnchorPane.leftAnchor="69.0" AnchorPane.rightAnchor="248.0" AnchorPane.topAnchor="107.0"/>
        <Label layoutX="69.0" layoutY="154.0" prefHeight="17.0" prefWidth="83.0" text="Password"
               AnchorPane.leftAnchor="69.0" AnchorPane.rightAnchor="248.0"/>
        <ButtonBar layoutX="121.0" layoutY="233.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="27.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#handleOk" text="Login"/>
                <Button mnemonicParsing="false" onAction="#handleCancel" text="Exit"/>
                <Button mnemonicParsing="false" onAction="#handleNew" text="New"/>
            </buttons>
        </ButtonBar>
        <PasswordField fx:id="Pwordfield" layoutX="201.0" layoutY="150.0"/>
    </children>
    <cursor>
        <Cursor fx:constant="DEFAULT"/>
    </cursor>
</AnchorPane>
